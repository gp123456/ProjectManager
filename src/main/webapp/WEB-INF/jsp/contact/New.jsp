<div id="new-contact">
    <h1>New Contact</h1>
    <div class="critDivs">
        <label style="font: icon;size: 12">Name</label>
        <input id="new contact-name" type="text"/>
    </div>
    <div class="critDivs">
        <label style="font: icon;size: 12">Surname</label>
        <input id="new contact-surname" type="text"/>
    </div>
    <div class="critDivs">
        <label style="font: icon;size: 12">Phone</label>
        <input id="new contact-phone" type="text"/>
    </div>
    <div class="critDivs">
        <label style="font: icon;size: 12">eMail</label>
        <input id="new contact-email" type="text"/>
    </div>
    <div class="critDivs">
        <label style="font: icon;size: 12">Vessel</label>
        <select id="new-contact-vessel" onchange="projectFilterVessel()"></select>
    </div>
    <div class="critDivs">
        <label style="font: icon;size: 12">Company</label>
        <select id="new-contact-company"></select>
    </div>
    <div class="critDivs">
        <label style="font: icon;size: 12">eMail</label>
        <input id="new contact-email" type="text"/>
    </div>
</div>